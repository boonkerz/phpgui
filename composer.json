{
  "name": "boonkerz/phpgui",
  "type": "library",
  "authors": [
    {
      "name": "Thomas Peterson",
      "email": "info@thomas-peterson.de"
    }
  ],
  "repositories": [
    {
      "type": "path",
      "url": "./libs/binaries-downloader"
    }
  ],
  "autoload": {
    "psr-4": {
      "PHPGui\\": "src/",
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPGui\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-ffi": "*",
    "ffi/env": "^1.0",
    "ffi/proxy": "^1.0",
    "ffi/location": "^1.0",
    "local/binaries-downloader": "@dev",
    "illuminate/container": "~10",
    "illuminate/config": "~10",
    "illuminate/support": "~10",
    "ffi/preprocessor": "^0.2.1|^1.0",
    "symfony/var-dumper": "^v6",
    "shieldon/simple-cache": "1.3.4",
    "symfony/finder": "^6.3",
    "symfony/filesystem": "^6.3",
    "sabberworm/php-css-parser": "^8.4",
    "webmozart/assert": "^1.11",
    "lkdevelopment/hetzner-cloud-php-sdk": "^2.6",
    "ext-parallel": "*",
    "zumba/json-serializer": "^3"
  },
  "config": {
    "allow-plugins": {
      "local/binaries-downloader": true,
      "bamarni/composer-bin-plugin": true
    }
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.8"
  }
}
